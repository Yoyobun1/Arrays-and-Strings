    "def prefix_sum(arr):\n",
    "    arr1=[0]*len(arr)\n",
    "    j=0\n",
    "    for i in range(len(arr)):\n",
    "        arr1[i]=arr1[i-1]+arr[i]\n",
    "    return arr1\n",
    "\n",
